{
  "name": "BuildingMomo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:secure": "vite --mode secure",
    "build": "vue-tsc -b && vite build && node scripts/build-i18n.js",
    "build:secure": "vue-tsc -b && vite build --mode secure && node scripts/build-i18n.js && node scripts/clean-seo-for-secure.js",
    "preview": "vite preview",
    "fetch-data": "node scripts/fetch-data.js",
    "prebuild": "npm run fetch-data",
    "prebuild:secure": "npm run fetch-data",
    "prepare": "husky",
    "deploy:cloudflare": "npm run build:secure && wrangler pages deploy dist --project-name buildingmomo-lab"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.8",
    "@tailwindcss/vite": "^4.1.17",
    "@tresjs/cientos": "^5.1.0",
    "@tresjs/core": "^5.1.0",
    "@vueuse/core": "^14.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "comlink": "^4.4.2",
    "idb-keyval": "^6.2.2",
    "lucide-vue-next": "^0.553.0",
    "pinia": "^3.0.4",
    "reka-ui": "^2.6.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "three": "^0.181.1",
    "three-mesh-bvh": "^0.9.4",
    "vue": "^3.5.22",
    "vue-sonner": "^2.0.9"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/three": "^0.181.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.1.14",
    "vite-plugin-pwa": "^1.2.0",
    "vue-tsc": "^3.1.0",
    "wrangler": "^4.54.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,json,md}": [
      "prettier --write"
    ]
  }
}
